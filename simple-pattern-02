#include <stdio.h>
#include <stdlib.h>
#define MAX 1000

int main(){
	 int n,i,j,k,l;
    scanf("%d", &n);
    printf("\n");
    for ( i = n; i > 0; --i)
       {
       	for (j = 0; j < i; ++j)
       	{
       		printf("%d",i );
       		
       	}
         	printf("\n");
       }
       for( i = 2; i <= n; ++i){
       	
       	for (k = 0; k < i; ++k)
       	{
       		printf("%d",i );
       	}
       	
       	printf("\n");
       }   
    return 0;  	

}

