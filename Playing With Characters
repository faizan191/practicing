#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define MAX 1000
int main() 
{
    char a,b[MAX],c[MAX];
    scanf("%c",&a);
    scanf("%s",b);
    scanf("\n");
    scanf("%[^\n]%*c", c);
    printf("%c \n",a);
    printf("%s \n",b);   
    printf("%s \n",c);     
    return 0;
}
